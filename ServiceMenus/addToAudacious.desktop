[Desktop Entry]
Type=Service
ServiceTypes=KonqPopupMenu/Plugin
MimeType=audio/*;video/*;inode/directory;
Actions=addToAudacious;
X-KDE-Priority=TopLevel

[Desktop Action addToAudacious]
Name=Add to Audacious now playing
Icon=/home/raitaro/IconsThemes/la-capitaine-dark/apps/scalable/audacious.svg
Exec=audacious --qt -E %U
